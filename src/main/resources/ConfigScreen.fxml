                        <ImageView fitHeight="31" fitWidth="31" pickOnBounds="true" preserveRatio="true">
                            <Image url="file:src/assets/images/weapon_blueberry.png" />
                        </ImageView>
                    </graphic>
                </Button>
                <Button onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" fx:id="honey" accessibleText="Honey" onAction="#setWeapon" prefHeight="46" prefWidth="46">
