                    <ImageView fitHeight="31" fitWidth="31" pickOnBounds="true" preserveRatio="true">
                        <Image url="file:src/assets/images/weapon_sword.png" />
                    </ImageView>
                </graphic>
                </Button>
                <Button onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" fx:id="blueberry" accessibleText="Blueberry" onAction="#setWeapon" prefHeight="46" prefWidth="46">
